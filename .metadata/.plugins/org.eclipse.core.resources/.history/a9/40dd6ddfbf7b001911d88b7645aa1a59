package hello;

import java.util.Scanner;

public class temperature {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int C = 0;
		System.out.print("请输入华氏温度：");
		C = in.nextInt();
		int F = 0;
		F = (int)(C*(9/5.0)+32);
		System.out.println("转换为摄氏温度为："+F);

	}

}
